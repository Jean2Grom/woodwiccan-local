{
    "admin": {
        "debug": false,
        "access": [
            "localhost/admin"
        ],
        "manage": [
            "*"
        ],
        "defaultContext": "standard",        
        "witches": {
            "root": {
                "match": 1,
                "craft": false,
                "parents": false,
                "sisters": false,
                "children": false
            },
            "arborescence": {
                "match": 1,
                "craft": false,
                "invoke": "arborescence",
                "parents": false,
                "sisters": false,
                "children": {
                    "depth": "*",
                    "craft": false
                }
            },
            "chooseWitch": {
                "match": 1,
                "craft": false,
                "invoke": "chooseWitch",
                "parents": false,
                "sisters": false,
                "children": {
                    "depth": "*",
                    "craft": false
                }
            },            
            "menu": {
                "match": 8,
                "craft": false,
                "children": {
                    "depth": 1,
                    "craft": false
                }
            },
            "target": {
                "match": "id", 
                "parents": {
                    "depth": 1,
                    "craft": false
                },
                "children": {
                    "depth": 1,
                    "craft": true
                }
            }, 
            "mother": {
                "match": "mother", 
                "craft": false,
                "conditions": {
                    "invoke": [
                        "view"
                    ]
                }
            }, 
            "origin": {
                "match": "origin", 
                "craft": false,
                "conditions": {
                    "invoke": [
                        "witch/clipboard"
                    ]
                },
                "children": {
                    "depth": "*",
                    "craft": false
                }
            }, 
            "destination": {
                "match": "dest", 
                "craft": false,
                "conditions": {
                    "invoke": [
                        "witch/clipboard"
                    ]
                },
                "children": {
                    "depth": 1,
                    "craft": false
                }
            }            
        },
        "modules": {
            "*": {
                "notAllowed": "login"
            },
            "login": {
                "defaultContext": "login"
            },
            "404": {
                "defaultContext": "login"
            },
            "arborescence": {
                "notAllowed": false,
                "navigationUnSafeMode": true
            },
            "chooseWitch": {
                "notAllowed": false
            }
        }
    },
    "blank": {
        "access": [
            "localhost",
            "localhost/blank"
        ], 
        "status": {
            "0": "public",
            "1": "admin"
        }
    }
}
    